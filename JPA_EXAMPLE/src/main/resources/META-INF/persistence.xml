		<class>com.av.domain.Project</class>
		<class>com.av.domain.Emp</class>
		<class>com.av.domain.ValidEmp</class>
<!-- 		<class>com.av.domain.ContractEmp</class>
		<class>com.av.domain.StaffEmp</class> -->

		<exclude-unlisted-classes>true</exclude-unlisted-classes>

	</persistence-unit>
</persistence>
